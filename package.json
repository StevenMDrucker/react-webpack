{
  "name": "react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Simple production ready boilerplate for React, Webpack (SASS and React hot reloading), tests and coverage.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "jest",
    "coverage": "jest --coverage",
    "dist": "cross-env NODE_ENV=production webpack -p --progress",
    "postinstall": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srn/react-webpack-boilerplate.git"
  },
  "engines": {
    "node": "5",
    "npm": "3"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "production",
    "sass",
    "hot",
    "reload"
  ],
  "author": "Søren Brokær <root@srn.io> (http://srn.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/srn/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/srn/react-webpack-boilerplate",
  "dependencies": {
    "babel-polyfill": "6.3.14",
    "babel-runtime": "6.3.19",
    "bibliography": "0.1.0",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "d3": "^4.2.7",
    "ejs": "2.4.1",
    "express": "4.13.4",
    "express-ejs-layouts": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fixed-data-table": "0.6.3",
    "imports-loader": "0.6.5",
    "lodash": "4.16.4",
    "react": "^15.3.2",
    "react-bootstrap": "0.30.5",
    "react-container-dimensions": "1.3.0",
    "react-dom": "^15.3.2",
    "react-imagebox": "1.0.0-beta",
    "react-masonry-component": "4.3.1",
    "react-measure": "1.4.5",
    "react-router": "3.0.0",
    "react-router-bootstrap": "0.23.1",
    "react-search-input": "0.10.3",
    "react-slick": "0.14.5",
    "react-stonecutter": "0.3.3",
    "webpack": "1.12.12"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.2.0",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.4.5",
    "babel-jest": "6.0.1",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "coveralls": "2.11.6",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "jest-cli": "0.8.2",
    "less": "2.7.1",
    "node-sass": "3.4.2",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "1.3.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testFileExtensions": [
      "test.js",
      "test.jsx"
    ],
    "testDirectoryName": "client"
  }
}
