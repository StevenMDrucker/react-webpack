@inproceedings{fisher2012trust,
	booktitle={Proceedings of the SIGCHI Conference on Human Factors in Computing Systems},
	tags={{'collaborators': ['Fisher', 'Popov', 'schraefel'], 'subject': ['UI', 'Information', 'Visualization', 'Big Data'], 'year': ['2012'], 'publication': ['SIGCHI']}},
