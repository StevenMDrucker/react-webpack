	abstract={We present Gestalt, a development environment designed tosupport the process of applying machine learning. While traditional programming environments focus on source code, we explicitly support both code and data. Gestalt allows developers to implement a classification pipeline, analyze data as it moves through that pipeline, and easily transition between implementation and analysis. An experiment shows this significantly improves the ability of developers to find and fix bugs in machine learning systems. Our discussion of Gestalt and our experimental observations provide new insight into general-purpose support for the  achine learning process.},
	caption={Gestalt}
}

@techreport{imageflow-streaming-image-search,
	link={https://www.microsoft.com/en-us/research/publication/imageflow-streaming-image-search/},
	id={33},
	author={Jampani, Varun and Ramos, Gonzalo and Drucker, Steven},
	month={November},
	institution={Microsoft Research},
	title={ImageFlow: Streaming Image Search},
	img={researchImages/imageflow.png},
	ID={imageflow-streaming-image-search},
	caption={Imageflow},
	year={2010},
	ENTRYTYPE={techreport},
	primary={UI-Information},
	abstract={{Traditional grid and list representations of image search results are the dominant interaction paradigms that users face on a daily basis, yet it is unclear that such paradigms are well-suited for experiences where the user's task is to browse images for leisure, to discover new information or to seek particular images to represent ideas. We introduce ImageFlow, a novel image search user interface that explores a different alternative to the traditional presentation of image search results. ImageFlow presents image results on a canvas where we map semantic features (e.g., relevance, related queries) to the canvas' spatial dimensions (e.g., x, y, z) in a way that allows for several levels of engagement Ã¢Â�Â� from passively viewing a stream of images, to seamlessly navigating through the semantic space and actively collecting images for sharing and reuse. We have implemented our system as a fully functioning prototype, and we report on promising, preliminary usage results.},},
	tags={{'collaborators': ['Ramos', 'Jampani'], 'subject': ['Media', 'UI', 'Web', 'Visualization'], 'year': ['2010'], 'publication': ['TechReport']}}
}

