@inproceedings{barnett2013stat,
	booktitle={Proceedings of the 2013 ACM SIGMOD International Conference on Management of Data},
	tags={{'collaborators': ['Barnett', 'Chandramouli', 'DeLine', 'Fisher', 'Goldstein', 'Morrison', 'Platt'], 'subject': ['Information'], 'year': ['2013'], 'publication': ['Sigmod']}},
	id={20},
