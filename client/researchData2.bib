@inproceedings{drucker2013touchviz,
	booktitle={Proceedings of the SIGCHI Conference on Human Factors in Computing Systems},
	tags={{'collaborators': ['Fisher', 'Sadana', 'Herron', 'schraefel'], 'subject': ['UI', 'Information', 'Visualization', 'Touch'], 'year': ['2013'], 'publication': ['SIGCHI']}},
	id={18},
